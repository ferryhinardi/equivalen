{
  "name": "www",
  "version": "0.1.0",
  "private": true,
  "author": "Ferry Hinardi",
  "description": "Apps Excellent Media",
  "homepage": "./",
  "repository": "https://bitbucket.org/ferry_hinardi/equivalent",
  "devDependencies": {
    "babel-eslint": "8.1.0",
    "concurrently": "^3.6.0",
    "electron": "^2.0.4",
    "electron-builder": "^20.20.4",
    "electron-builder-squirrel-windows": "^20.20.4",
    "eslint": "4.14.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-react": "7.5.1",
    "foreman": "^3.0.1",
    "react-scripts": "1.1.4",
    "wait-on": "^2.1.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "prerelease",
    "@fortawesome/free-solid-svg-icons": "prerelease",
    "@fortawesome/react-fontawesome": "prerelease",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "electron-is-dev": "^0.3.0",
    "electron-updater": "^3.0.3",
    "express": "^4.16.3",
    "flow-bin": "^0.77.0",
    "flow-typed": "^2.5.1",
    "node-machine-id": "^1.1.10",
    "platform": "^1.3.5",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-art": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-native": "^0.56.0",
    "react-native-electron": "^0.7.0",
    "react-native-web": "^0.8.9",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "main": "build/electron.js",
  "build": {
    "appId": "com.gps.equivalent",
    "files": [
      "./build/**/*",
      "./build/electron.js"
    ],
    "dmg": {
      "contents": [
        {
          "x": 281,
          "y": 249,
          "type": "file"
        },
        {
          "x": 532,
          "y": 249,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "https://cdn2.iconfinder.com/data/icons/designer-skills/128/react-256.png"
    },
    "directories": {
      "buildResources": "public"
    }
  },
  "scripts": {
    "dev": "nf start -p 3000",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/electron.js",
    "electron-win": "build --win --x64",
    "publish": "yarn run electron-pack && yarn run electron-win",
    "ship": "build --win -p always",
    "test:flow": "node_modules/flow-bin/cli.js"
  },
  "forceCodeSigning": true
}
